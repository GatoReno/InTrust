<div class="container p-4">
    <div class="row">
        {{#if links}}
        <div class="col-lg-1 col-md-1"></div>
        <div class="col-lg-10 col-md-10 mx-auto ">

            <div class="card " style="width: 100%;">
                <div class="card-header">
                    <h3>Info de Proyecto {{links.title}}</h3>
                </div>

                <div class="card-body">

                    <h1>{{user.id_user}}</h1>
                    <form action="" method="POST ">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th scope="col">...</th>
                                    <th scope="col">...</th>
                                    <th scope="col">...</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>Mail </td>
                                    <td>{{links.mail}}</td>
                                    <td></td>
                                </tr>
                                <tr>
                                    <td>Telefono </td>
                                    <td>{{links.phone}}</td>
                                    <td></td>
                                </tr>
                                <tr>
                                    <td>Mail </td>
                                    <td>{{links.mail}}</td>
                                    <td></td>
                                </tr>
                                <tr>
                                    <td>Nombre de investigador </td>
                                    <td>{{links.name_investigator}}</td>
                                    <td></td>
                                </tr>
                                <tr>
                                    <td>Mail de investigador </td>
                                    <td>{{links.mail_investigator}}</td>
                                    <td></td>
                                </tr>
                                </tr>
                                <tr>
                                    <td>Mail de investigador </td>
                                    <td>{{links.cvu}}</td>
                                    <td></td>
                                </tr>
                                <tr>
                                    <td></td>
                                    <td></td>
                                    <td>
                                        <input type="submit" class="btn btn-default pull-right" value="Guardar">
                                    </td>
                                </tr>

                            </tbody>
                        </table>
                    </form>

                </div>
            </div>

        </div>
        <div class="col-lg-1 col-md-1"></div>
        {{else}}

        {{/if}}

    </div>

    <div class="row">
        {{#if admin}}
        <div class="col-lg-1 col-lg-1"></div>
        <div class="col-lg-10 col-md-10 mx-auto ">

            <div class="card " style="width: 100%;">
                <div class="card-header">
                    <h3>Info de Administrador {{admin.name}}</h3>
                </div>

                <div class="card-body">

                    <h1>{{user.id_user}}</h1>
                    <form action="" method="POST ">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th scope="col">...</th>
                                    <th scope="col">...</th>
                                    <th scope="col">...</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>Mail </td>
                                    <td>
                                        {{admin.mail}}
                                    </td>
                                    <td><input type="mail" name="mail" value="{{admin.mail}}"></td>
                                </tr>
                                <tr>
                                    <td>Contraseña </td>
                                    <td> {{admin.pass}}</td>
                                    <td><input type="password" name="pass" placeholder="nueva contraseña"></td>
                                </tr>
                                <tr>
                                    <td></td>
                                    <td> </td>
                                    <td>
                                        <input type="password" name="pass2" placeholder="repita su contraseña">
                                    </td>
                                </tr>
                                <tr>
                                    <td></td>
                                    <td></td>
                                    <td>
                                        <input type="submit" class="btn btn-default pull-right" value="Guardar">
                                    </td>
                                </tr>

                            </tbody>
                        </table>
                    </form>

                </div>
            </div>

        </div>
        <div class="col-lg-1 col-lg-1"></div>

        {{else}}

        {{/if}}

    </div>
</div>